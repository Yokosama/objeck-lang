use Collection.Generic;

class Test {
	function : Main(args : String[]) ~ Nil {
		list := DoRep(4, 5);
		while(list->More()) {
			list->Get()->Get()->PrintLine();
			list->Next();
		};
		Sum(list)->Get()->PrintLine();
	}

	function : DoRep(x : Int, n : Int) ~ CompareList<IntHolder> {
		if(n = 0) {
			return CompareList->New()<IntHolder>;
		}
		else {
			y := CompareList->New()<IntHolder>;
			y := DoRep(x, n - 1);
			y->AddBack(x);
			return y;
		};
	}

	function : Sum(list : CompareList<IntHolder>) ~ IntHolder {
		if(list->IsEmpty()) {
			return 0;
		}
		else {
			return list->Front() + Sum(list->Rest()<IntHolder>);
		};
	}
}