use Collection.Generic;

class Capture {
	function : Main(args : String[]) ~ Nil {
		 funcs := Vector->New()<FuncHolder<IntHolder>>;
		 
		 for(i := 0; i < 10; i += 1;) {
		 	funcs->AddBack(FuncHolder->New(\() ~ IntHolder : () => i->Factorial() * funcs->Size())<IntHolder>);
		 };

		 each(i : funcs) {
		 	func := funcs->Get(i)->Get();
		 	func()->Get()->PrintLine();
		 };
	}
}