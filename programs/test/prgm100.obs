use Collection.Generic;

class FirstClass {
   function : Main(args : String[]) ~ Nil {
      fl := CompareVector->New()<FloatHolder>;
      fl->AddBack(1.25); fl->AddBack(3.5); fl->AddBack(6.66);

      il := Vector->New()<FloatHolder>;
      il->AddBack(101); il->AddBack(321); il->AddBack(950);
      
      zipped := Map->Zip(fl, il)<FloatHolder, FloatHolder>;
      zipped->Each(\^(a, b) => {
         if(a <> Nil & b <> Nil) {
            a->PrintLine();
            b->PrintLine();
         };
      }, 10);
   }
}
