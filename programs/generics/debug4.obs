#~
	testing:
		* nested method calls
~#

class Test {
	function : Main(args : String[]) ~ Nil {
		v : Bar<String> := Bar->New("Foo Bar")<String>;
		v->Get()->SubString(5)->PrintLine();
	}
}

class Bar<V> {
	@v : V;
	
	New() {}

	New(v : V) {
		@v := v;
	}

	function : Instance() ~ Bar {
		return Bar->New()<V>;
	}
	
	method : public : Set(v : V) ~ Nil {
		@v := v;
	}
	
	method : public : Get() ~ V {
		return @v;
	}
}