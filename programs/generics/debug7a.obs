#~
	testing:
		* generics with boxing
~#

class Foo {
	New() {}
}

class Bar<V> {
	@v : V;
	
	New() {}

	New(v : V) {
		@v := v;
	}

	method : public : Apply(f : (V) ~ V) ~ Nil {
		@v := f(@v);
	}
	
	method : public : Set(v : V) ~ Nil {
		@v := v;
	}
	
	method : public : Get() ~ V {
		return @v;
	}
}